{
  "name": "open-next-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "cloudflare && wrangler deploy",
    "preview": "cloudflare && wrangler dev",
    "drizzle:update-sqlite": "drizzle-kit generate --out ./src/db/migrations --schema ./src/db/schema.ts --dialect sqlite",
    "drizzle:update-postgres": "drizzle-kit generate --out ./src/db/postgre-migrations --schema ./src/db/schema.ts --dialect postgresql",
    "drizzle:migrate": "wrangler d1 migrations apply open-next-test-db --remote",
    "drizzle:migrate-postgres": "drizzle-kit migrate",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "lucide-react": "^0.468.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@opennextjs/cloudflare": "^0.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^3.95.0"
  }
}
